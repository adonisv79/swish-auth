    "express-swish-protocol": "1.4.1",
    "helmet": "3.22.0",
    "http-error-types": "^1.0.1",
    "ioredis": "4.16.1",
    "mongoose": "5.9.9",
    "rate-limiter-flexible": "2.1.3",
    "redis": "3.0.2",
    "redis-toolbox": "1.0.1",
    "uuid": "7.0.3",
    "winston": "3.2.1"

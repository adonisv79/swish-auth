{
  "name": "swish-auth",
  "version": "0.2.0",
  "description": "Authentication service designed with data privacy and control as top priority",
  "main": "index.js",
  "scripts": {
    "clonestatic": "cp -r ./src/public ./dist/src/public",
    "postinstall": "tsc",
    "build": "tsc && npm run clonestatic",
    "start": "node dist/src/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --ext .ts,.tsx --fix",
    "debug:dev": "ts-node src/index.ts",
    "build:dockerdev": "build.bat dev",
    "build:dockerprod": "build.bat prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisv79/swish-auth.git"
  },
  "keywords": [
    "swish",
    "authentication",
    "authorization",
    "security",
    "users",
    "roles",
    "data",
    "privacy"
  ],
  "author": "Adonis Lee Villamor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisv79/swish-auth/issues"
  },
  "homepage": "https://github.com/adonisv79/swish-auth#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^16.0.12",
    "@types/helmet": "0.0.45",
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^25.1.4",
    "@types/joi": "^14.3.4",
    "@types/mongoose": "^5.7.11",
    "@types/node": "^13.9.5",
    "@types/redis": "^2.8.17",
    "@types/uuid": "^7.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.2.3",
    "snyk": "^1.305.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
